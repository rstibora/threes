server {
    listen 80;
    server_name 164.90.218.235;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/threes/threes;
    }

    location /app/ {
        alias /home/threes/threes/static/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}
